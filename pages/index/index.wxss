.contain{
   padding: 10rpx 20rpx;
}
.block{
  margin:20px 0px;
}
.search{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
 
}
.searchInput{
  flex: 8;
  border: 1rpx solid #888;
  font-size: 18px;
  padding: 6px;
}
.searchBtn{
  flex: 2;
  /* border: 1rpx solid #888; */
  background: rgb(2, 175, 255);
  color: white;
  font-size: 14px;
}
.title{
  font-size: 16px;
  color: #333
}
.module{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.moduleItem{
  display: flex;
  flex-direction: column;
  text-align: center;
  align-items: center;
}
.moduleItem image{
  width: 80px;
  height: 80px;
}
.guessLike{
  /* margin: auto;
  text-align: center; */
  font-size:12px;
  color: #888;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  text-align: center
}
.guessLikeItem{
  display: inline-block;
  /* background: red; */
  border: 1px solid #888;
  margin:5px;
}
.guessLikeItem image,.musicProduce image{
  width: 60px;
  height: 70px
}
.musicProduce{
  font-size:12px;
  color: #888;
  margin: auto;
  text-align: center;
}
.musicProduce .musicProduceItem{
   display: inline-block;
  /* background: red; */
  border: 1px solid #888;
  margin:10px;
}


.playControll{
  position: fixed;
  -webkit-user-drag: drag;
  z-index: 999;
  bottom: 50px;
  right: 0px;
  color: white;
  font-size: 16px;
  padding: 12px 20px;
  background: rgb(2, 175, 255);
  border-top-left-radius:20px;
  border-bottom-left-radius: 20px; 
  margin-right: -70px;
}
